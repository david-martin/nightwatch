<?xml version="1.0" encoding="UTF-8" ?>
<testsuites name="<%= moduleName %>"
             errors="<%= errors %>"
             failures="<%= failures %>"
             tests="<%= testsNo %>">
  <testsuite name="<%= moduleName %>" errors="" failures="" hostname="" id="" skipped="" tests="" time="" timestamp="">
  <% for (var testsuite in module) { %>
    <testcase name="<%= testsuite %>.js">
      <% var tests = module[testsuite].tests %>
      <% for (var i = 0; i < tests.length; i++) { %>
        <% if (tests[i].failure) { %>
          <failure message="<%= tests[i].failure %>"></failure>
        <% } %>
      <% } %>
      <system-out>
<%= moduleName %>.js "<%= testsuite %>"<% for (var i = 0; i < tests.length; i++) { %><% if (tests[i].message) { %>
<%= tests[i].message %><% } %><% } %>
      <% for (var i = 0; i < tests.length; i++) { %>
        <% if (tests[i].stacktrace) {%><%= tests[i].stacktrace %><% } %>
        <% if (tests[i].screenshots && tests[i].screenshots.length > 0) { %>
          <% for (var j = 0; j < tests[i].screenshots.length; j++) { %>[[ATTACHMENT|<%= tests[i].screenshots[j] %>]]<% } %>
        <% } %>
      <% } %>
      </system-out>
    </testcase>
    <% if (systemerr != '') {%>
    <system-err>
      <%= systemerr %>
    </system-err>
    <% } %>
  <% } %>
  </testsuite>
</testsuites>
